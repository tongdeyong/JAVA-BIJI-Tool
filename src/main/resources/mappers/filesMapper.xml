        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.deyong.javalearning.dao.IFilesMapper">
	<insert id="insertFileMsg" parameterType="com.deyong.javalearning.service.entity.FilesMsg">
